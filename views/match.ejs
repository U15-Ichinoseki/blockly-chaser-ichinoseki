<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>Blockly CHaser</title>
  <link rel="icon" href="/images/favicon.ico">
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <link rel='stylesheet' href='/stylesheets/animation.css' />
  <link rel='stylesheet' href='/stylesheets/menu.css' />
  <link rel='stylesheet' href='/stylesheets/game-board.css' />
  <link rel='stylesheet' href='/stylesheets/match.css' />

  <script src="/socket.io/socket.io.js"></script>
  <script src="/javascripts/jquery-3.4.1.min.js"></script>
  
  <script src="/javascripts/howler.min.js"></script>
  
  <script src="/javascripts/match.js"></script>
  <script src="/javascripts/game_board.js"></script>
  
  
</head>
<body  class="animation_stop">
  <div class="header">
    
    <div class = "back_page">
      <div class = "back_button"><a href="./menu-match"></a></div>
    </div>
    <div class="control_area">
      <div class = "page_title">
        <h1><span class = "page_title_span">対戦</span><div id = "server_name"></div></h1>
      </div>
      <div class = "control_button">
        <button id="fullscreenButton" onclick="reqFullscreen('contents_area')" class="notext" title="最大化"></button>
      </div>
    </div>  
    
  </div>
  <div id="contents_area">
    <div id="ready_area">
      <div id="cool_ready_title">cool</div>
      <div id="cool_ready">
        <iframe id="cool_player_iframe" class="player_iframe"></iframe>
      </div>
      <div id="hot_ready_title">hot</div>
      <div id="hot_ready">
        <iframe id="hot_player_iframe"  class="player_iframe"></iframe>
      </div>
      <div id="game_start" class="display_init">ゲームスタート</div>
    </div>
    <div id="game_area" class="display_off">
      <div id="game_board"></div>
      <div id="game_info"></div>
    </div>
  </div>
  

</body>
</html>